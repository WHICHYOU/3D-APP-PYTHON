# Example environment variables
# Copy this file to .env and fill in your values

# License Configuration
LICENSE_SECRET_KEY=your_secret_key_here  # Generate with: python -c "import secrets; print(secrets.token_urlsafe(32))"

# License Server Configuration
LICENSE_SERVER_URL=https://api.2d3dconverter.com/v1
LICENSE_API_KEY=your_api_key_here

# Analytics (opt-in)
ENABLE_ANALYTICS=false
ANALYTICS_ENDPOINT=https://analytics.2d3dconverter.com

# Development
DEBUG_MODE=false
LOG_LEVEL=INFO

# GPU Configuration
FORCE_CPU=false
GPU_DEVICE=0

# Model Downloads
MODEL_DOWNLOAD_URL=https://models.2d3dconverter.com
